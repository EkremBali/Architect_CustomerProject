@model AlertMessage

<div id="AlertMessageDiv" class="row">
	<div class="col-md-12">
		<div class="alert alert-@Model.AlertType">
			<h4 class="alert-title">@Model.Title</h4>
			<p>@Model.Message</p>
		</div>
	</div>
</div>

<script>
	var alertMessage = document.getElementById("AlertMessageDiv");

	setTimeout(() => {
		alertMessage.classList.add("d-none");
	},3000);
	
</script>